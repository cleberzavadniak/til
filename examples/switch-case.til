set offset 10
range 0 10 | transform x {
        return $x [math ($x + $offset)]
    } | case (>x 10) {
        io.out "$x + $offset is ten"
    } | case (>x 11) {
        io.out "$x + $offset is eleven"
    } | case (>x >y) {
        io.out "$x + $offset is $y"
        break
    } | case (>x >y) {
        io.out "$x + 10 is $y"
        io.out "BUT THIS MESSAGE SHOULD NOT BE DISPLAYED!"
    }

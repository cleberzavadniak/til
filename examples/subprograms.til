set x 11
set y 22

proc f {x y z} {
    {$x $y $z} |> std.out
    std.out {$x $y $z}
    return $x $y $z
}
proc g {x y z} {
    return {$x $y $z}
}
proc h {x y z} {
    return "$x $y $z"
}

set result1 [f $x $y 33]
{$result1} |> std.out

set result2 [g $x $y 33]
{$result2} |> std.out

set result3 [h $x $y 33]
{$result3} |> std.out

f 1 [expr {$x+1}] {3 4 {5 "$y $z"}} |> reverse [expr $x+2] |> std.out
set x "std.out" |> $x

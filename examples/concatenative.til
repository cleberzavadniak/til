import std.io as io
import std.stack as s


foreach (word) ("arara" "ovo" "algo") {
    io.out $word
    s.dup $word
    s.reverse
    if ([s.equals?]) {
        io.out "$word is a palindrome"
    } else {
        io.out "$word is NOT a palindrome"
    }
}

io.out "---"

io.out [s.dup "duplicate!"]
io.out [
    s.dup "revert it"
    s.reverse
]
io.out [
    s.dup "revert it"
    s.reverse
    s.equals?
]
